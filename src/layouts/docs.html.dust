<!DOCTYPE html>
<html>
<head lang="en">
	<meta charset="UTF-8">
	<title>{>"partial/meta/page-title"/}</title>

	<!-- Stylesheets -->

	<link rel="stylesheet" href="/css/grits-docs.css" />

	{?plugins.grits-plugin-docs.layoutConfig.docs.custom-css}
	<!-- Custom JS -->
	{#plugins.grits-plugin-docs.layoutConfig.docs.custom-css}
	<link rel="stylesheet" href="{.}" />{/plugins.grits-plugin-docs.layoutConfig.docs.custom-css}{/plugins.grits-plugin-docs.layoutConfig.docs.custom-css}


	<!-- End Stylesheets -->

</head>
<body>

{>"partial/menu/navbar"/}

<div id="doc-content-outer" class="container-fluid">
	<div id="doc-content-row" class="row">

		<div id="section-doc-menu" class="col-md-3 doc-menu menu-toc hidden-print">
			{>"partial/menu/toc"/}
		</div>

		<div id="doc-content-inner" class="col-md-9 doc-body">

			{>"partial/print/header"/}
			{+bodyContent /}
			{>"partial/print/footer"/}

			{?plugins.grits-plugin-docs.helpers.disqus}
					<!--There is a disqus helper configured in the conf-->
				{@disqus
					{#plugins.grits-plugin-docs.helpers.disqus}
						shortname="{shortname}"
					{/plugins.grits-plugin-docs.helpers.disqus}
				/}
			{/plugins.grits-plugin-docs.helpers.disqus}

		</div>

	</div>
</div>

<!-- JS -->

	<script type="text/javascript" src="/js/jquery.min.js"></script>
	<script type="text/javascript" src="/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="/js/toc.js"></script>
	<script type="text/javascript" src="/js/imarkup.js"></script>

{?plugins.grits-plugin-docs.layoutConfig.docs.custom-js}
<!-- Custom JS -->
{#plugins.grits-plugin-docs.layoutConfig.docs.custom-js}
<script type="text/javascript" src="{.}"></script>{/plugins.grits-plugin-docs.layoutConfig.docs.custom-js}{/plugins.grits-plugin-docs.layoutConfig.docs.custom-js}


<!-- End JS -->

</body>
</html>
